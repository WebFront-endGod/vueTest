   <div class=\"title\">\n                       <p>{{item.title}}</p>\n                       <p>{{item.text}}</p>\n                   </div>\n                   <div class=\"imgs\">\n                       <img :src=\"item.imgUrl\" alt=\"\"/>\n                   </div>\n               </router-link>\n            </ul>\n            <!--右侧-->\n             <ul :class=\"six\">\n                <router-link tag=\"li\" to=\"/\" v-for=\"(item,index) in Data2\" :key=\"index\">\n                    <div class=\"title\">\n                        <p>{{item.title}}</p>\n                        <p>{{item.text}}</p>\n                    </div>\n                    <div class=\"imgs\">\n                        <img :src=\"item.imgUrl\" alt=\"\"/>\n                    </div>\n                </router-link>\n             </ul>\n        </div>\n    </section>\n</template>\n\n<script>\n    export default {\n        props:['datas'],\n        computed:{\n            //获取左侧数组\n            Data1(){\n                const Data = (this.datas).Mun;\n                if(Data.length) {\n                  const Datas = Data.slice(0,2);\n                    return Datas\n                }\n            },\n            // 获取右侧数组\n            Data2(){\n                const Data = (this.datas).Mun;\n                if(Data.length) {\n                    const Datas = Data.slice(2,Data.length+1);\n                    return Datas\n                }\n            },\n\n            // 当两个数组长度值为真时显示此组件\n            isShow(){\n                 if(this.Data1.length&&this.Data2.length){\n                     return true\n               }\n\n            },\n\n            //根据右侧数组长度判断右侧显示的方式\n            six(){\n                if(this.Data2.length>2){\n                    return 'six'\n                }else{\n                    return 'sixs'\n                }\n\n\n\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"scss\">\n    .Lattice{\n        .pro_banner{\n            width: 100%;\n            height: 31px;\n            font-size: 16px;\n            text-align: center;\n            color: #3ca7df;\n            background-image: url(\"img/floor-blue.png\");\n            background-size: 100% 100%;\n            background-repeat:no-repeat ;\n            line-height: 31px;\n            font-weight: bold\n        }\n        .box {\n            display: flex;\n            ul{\n                flex: 1;\n                overflow: hidden;\n                box-sizing: border-box;\n                li{\n                    overflow: hidden;\n                    box-sizing: border-box;\n                    border-bottom: 1px solid #e8e8e8;\n                    height: 10em;\n                    padding-bottom: 4px;}\n                li:last-child{border-bottom: none}\n             }\n             ul:nth-child(1){\n                border-right: 1px solid #e8e8e8;\n            }\n            .six{\n                li{\n                    position: relative;\n                    height: 5em;\n                    padding-bottom: 2px;\n                }\n                .title{\n                    width: 45%;\n                }\n                .imgs{\n                width: 4.37em;\n                right: 6.4%;\n                position: absolute;\n                top: 4px;\n                margin-right: 0;\n              }\n\n            }\n\n        }\n        .title{margin-left:7.6%}\n        .title p:nth-child(1){\n           font-size: 14px;\n           padding-top: 0.68em;\n           line-height: 1em;\n           height: 1em;\n           overflow: hidden;\n        }\n        .title p:nth-child(2){\n           font-size: 12px;\n           margin-top: 0.3em;\n           margin-bottom: 0.2em;\n           color: #898989;\n           line-height: 1em;\n           height: 1em;\n           verflow: hidden;\n        }\n        .imgs{\n          width: 7em;\n          margin-right: 6.4%;\n          float: right;\n          img{\n            width: 100%;\n            height: 100%;\n          }\n\n        }\n\n    }\n\n</style>"]}]}